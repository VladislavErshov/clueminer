    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>nbm-maven-plugin</artifactId>
                <extensions>true</extensions>
                <configuration>
                    <publicPackages>
                        <publicPackage>org.clueminer.chart</publicPackage>
                        <publicPackage>org.clueminer.chart.overlay</publicPackage>
                        <publicPackage>org.clueminer.dialogs</publicPackage>
                        <publicPackage>org.clueminer.factory</publicPackage>
                        <publicPackage>org.clueminer.timechart</publicPackage>
                        <publicPackage>org.clueminer.timeseries</publicPackage>
                        <publicPackage>org.clueminer.timeseries.chart</publicPackage>
                        <publicPackage>org.clueminer.timeseries.utils</publicPackage>
                    </publicPackages>
                </configuration>
            </plugin>
        </plugins>
